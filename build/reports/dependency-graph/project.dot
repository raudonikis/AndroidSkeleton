  node [style=filled, fillcolor="#bbbbbb"];
  rankdir=TB;

  # Projects

  ":app" [fillcolor="#baffc9"];
  ":libraries:common" [fillcolor="#baffc9"];
  ":libraries:network" [fillcolor="#baffc9"];

